#!/usr/bin/bash

echo running ns2dccg Re=100 T=15 ...
./ns2dccg 100 15
echo running ns2dccg Re=400 T=25 ...
./ns2dccg 400 25
echo running ns2dccg Re=1000 T=25 ...
./ns2dccg 1000 25
echo running ns2dccg Re=3200 T=50 ...
./ns2dccg 3200 50

echo running ns2dccg256 Re=3200 T=50 ...
./ns2dccg256 3200 50
echo running ns2dccg256 Re=5000 T=50 ...
./ns2dccg256 5000 50
echo running ns2dccg256 Re=7500 T=75 ...
./ns2dccg256 7500 75
echo running ns2dccg256 Re=10000 T=100 ...
./ns2dccg256 10000 100
